<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="29" />




    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Finalproject"
        tools:targetApi="31">

        <activity
            android:name=".WelcomeActivity"
            android:exported="true"
            android:theme="@style/Theme.Finalproject">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject" />

        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject" />

        <activity
            android:name=".SignupActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject" />

        <activity
            android:name=".MainScreenActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject" />

        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".GoogleLoginActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".ResetPasswordActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".ForgotPasswordActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".UploadFileActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".FileDetailActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".ReportActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".ChangePasswordActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".EditProfileActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".PremiumActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".TrialActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".MonthlyActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".YearlyActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>
        <activity
            android:name=".ThankYouActivity"
            android:exported="false"
            android:theme="@style/Theme.Finalproject"/>



        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>

    </application>

</manifest>
